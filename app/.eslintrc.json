{
  "extends": ["react-app", "eslint:recommended", "google"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "sourceType": "module"
},
  "rules": {
	"linebreak-style": "off",
  "require-jsdoc": "off",
  "max-len": "off",
  "prefer-spread": "off"
  }
}
